{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "baseUrl": "./",
    "rootDir": "./src",
    "declaration": false,
    "skipLibCheck": true,
    "strict": true,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
  },
  "include": [
    "./src/*.ts",
    "./*.config.js",
  ],
  "exclude": [
    "node_modules",
  ],
}
