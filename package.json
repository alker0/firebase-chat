{
  "name": "firebase-chat",
  "version": "0.0.1",
  "author": "alker",
  "license": "MIT",
  "private": true,
  "browserslist": [
    "> 2%",
    "not dead"
  ],
  "targets": {
    "app": {
      "context": "browser",
      "engines": {
        "browsers": [
          "> 2%",
          "not dead"
        ]
      },
      "includeNodeModules": {
        "inferno": false,
        "css-zero/macro": false,
        "firebase/app": false,
        "firebase/database": false,
        "firebase/storage": false
      }
    }
  },
  "alias": {
    "lib": "~/src/lib",
    "components": "~/src/components",
    "/__/firebase": "https://www.gstatic.com/firebasejs"
  },
  "distDir": "build",
  "scripts": {
    "start": "run-p start:firebase start:parcel",
    "start:parcel": "parcel --port 8080 --target app src/templates/*.html",
    "start:firebase": "node ./scripts/firebase-proxy.js",
    "test": "jest",
    "citest": "CI=true jest --reporters=default --reporters=jest-github-action-reporter",
    "build": "parcel build .",
    "page:add": "node ./scripts/add-page.js",
    "page:delete": "node ./scripts/delete-page.js"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@parcel/config-default": "^2.0.0-alpha.3",
    "@parcel/validator-typescript": "^2.0.0-alpha.3",
    "@types/classnames": "^2",
    "babel-plugin-inferno": "^6.1.1",
    "babel-plugin-macros": "^2.8.0",
    "connect": "^3.7.0",
    "firebase-tools": "^8.6.0",
    "inferno": "^7.4.2",
    "jest": "^26.1.0",
    "jest-github-actions-reporter": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-beta.1",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-import-url": "^5.1.0",
    "postcss-ts-classnames": "^0.1.2",
    "posthtml-expressions": "^1.4.5",
    "posthtml-extend": "^0.3.0",
    "posthtml-include": "^1.4.3",
    "prettier": "^2.0.5",
    "purgecss": "^2.3.0",
    "purgecss-from-js": "^1.0.0",
    "squirrelly": "^8.0.3",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "superstatic": "^6.0.4",
    "typescript": "^3.9.7",
    "websocket-driver": "^0.7.4"
  },
  "dependencies:about": {
    "module: mergerino": "merge Object",
    "module: inferno": "DOM renderer",
    "module: classnames": "function of CSS class name",
    "module: firebase": "Firebase",
    "dev-tool: @babel/core": "JS transformer",
    "dev-tool: babel-plugin-macros": "JS macro",
    "dev-tool: babel-plugin-inferno": "JSX to Inferno JS",
    "dev-tool: @babel/preset-env": "Modern JS to ES5",
