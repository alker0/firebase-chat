{
  "name": "firebase-chat",
  "version": "0.0.1",
  "author": "alker",
  "license": "MIT",
  "private": true,
  "browserslist": [
    "> 2%",
    "not dead"
  ],
  "targets": {
    "app": {
      "context": "browser",
      "engines": {
        "browsers": [
          "> 2%",
          "not dead"
        ]
      },
      "includeNodeModules": {
        "inferno": false,
        "firebase/app": false,
        "firebase/database": false,
        "firebase/storage": false
      }
    }
  },
  "alias": {
    "lib": "~/src/lib",
    "components": "~/src/components",
    "/__/firebase": "https://www.gstatic.com/firebasejs"
  },
  "distDir": "build",
  "scripts": {
    "start": "run-s start:snowpack",
    "start:snowpack": "snowpack dev --port 8080",
    "build": "snowpack build",
    "test": "jest",
    "citest": "CI=true jest --reporters=default --reporters=jest-github-action-reporter",
    "rdb:build": "ts-node -sT ./scripts/database-rules-build database.rules.json",
    "rdb:emulator:get:rule": "node ./scripts/database-emulator-access -a -p '/.settings/rules.json?ns=talker-v1'",
    "rdb:emulator:put:rule": "ts-node -sT ./scripts/database-rules-build $HOME/bind/firebase-emulator/rules/talker-v1.rules.json",
    "page:add": "node ./scripts/add-page.js",
    "page:delete": "node ./scripts/delete-page.js"
  },
  "devDependencies": {
    "@alker/solid-js-types": "file:./src/lib/packages/solid-js-types",
    "@alker/solid-styled-tsx": "file:./src/lib/packages/solid-styled-tsx",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@firebase/app": "^0.6.11",
    "@firebase/component": "^0.1.19",
    "@firebase/rules-unit-testing": "^1.0.8",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@snowpack/plugin-babel": "^2.1.3",
    "@snowpack/plugin-build-script": "^2.0.10",
    "@snowpack/plugin-postcss": "^1.0.5",
    "@snowpack/plugin-run-script": "^2.1.6",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.10",
    "@types/snowpack-env": "^2.3.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-solid": "^0.21.0",
    "connect": "^3.7.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "firebase": "^7.24.0",
    "firebase-admin": "^9.2.0",
    "firebase-tools": "^8.12.1",
    "jest": "^26.5.3",
    "jest-github-actions-reporter": "^1.0.2",
    "long": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.2",
    "postcss-cli": "^8.1.0",
    "postcss-import": "^12.0.1",
    "postcss-import-url": "^5.1.0",
    "postcss-ts-classnames": "^0.2.0",
    "posthtml": "^0.13.4",
    "posthtml-cli": "^0.8.0",
    "posthtml-expressions": "^1.5.0",
    "posthtml-extend": "^0.5.0",
    "posthtml-include": "^1.5.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "purgecss": "^3.0.0",
    "purgecss-from-js": "^1.0.0",
    "querystring": "^0.2.0",
    "snowpack": "^2.15.0-pre.6",
    "source-map-support": "^0.5.19",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "superstatic": "^7.0.1",
    "supports-color": "^7.2.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "type-filtered-clsx": "file:./src/lib/packages/type-filtered-clsx",
    "typescript": "^4.0.3",
    "url": "^0.11.0",
    "websocket-driver": "^0.7.4"
  },
